
// generator → What Prisma generates (usually the Prisma Client).
generator client {
  provider = "prisma-client-js"
}

// datasource → Where Prisma connects (DB type + URL).
datasource db {
  provider = "postgresql" // mysql | sqlite | mongodb
  url      = env("DATABASE_URL")
}

// model → Represents a table/collection.
model FileStructure {
  id    Int    @id @default(autoincrement())
  name  String 
}






